{
    "name": "goennounce/laravel-email-templates",
    "description": "Laravel 5 database driven email templating",
    "type": "library",
    "authors": [
        {
            "name": "Jon Braud",
            "email": "jon@jdtsoftware.co.uk"
        },
        {
            "name": "Peter Harlacher",
            "email": "pm@helvetian.io"
        }
    ],
    "require": {
        "illuminate/contracts": "^5.6",
        "illuminate/database": "^5.6",
        "illuminate/mail": "^5.6",
        "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GoEnnounce\\LaravelEmailTemplates\\ServiceProvider"
            ],
            "aliases": {
                "EmailTemplate": "GoEnnounce\\LaravelEmailTemplates\\Facades\\EmailTemplates"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "GoEnnounce\\LaravelEmailTemplates\\": "src/"
        }
    }
}
